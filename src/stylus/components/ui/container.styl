// Container
.container
    padding-left s1
    padding-right s1
    margin-left auto
    margin-right auto
    box-sizing border-box
    max-width 100%

    @media sm-up
        max-width 540px

    @media md-up
        max-width 720px

    @media lg-up
        max-width 960px

    @media xl-up
        max-width 1140px
